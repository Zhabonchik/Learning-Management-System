{
  "xsappname": "learning-management-system",
  "description": "Roles for LMS app",
  "tenant-mode": "shared",
  "oauth2-configuration":{
    "redirect-uris": [
      "https://*.cfapps.us10-001.hana.ondemand.com/login/callback"
    ]
  },
  "scopes": [
    {
      "name": "$XSAPPNAME.Info",
      "description": "Get access to /application-info endpoint."
    }
  ],
  "role-templates": [
    {
      "name": "Admin",
      "description": "Get access to /application-info endpoint.",
      "scope-references": [
        "$XSAPPNAME.Info"
      ]
    }
  ]
}